{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions",
            "not ie <= 8"
          ]
        }
      }
    ],
    "stage-2"
  ],
  "plugins": [
    [
      "import",
      {
        "libraryName": "szyh-view-ui",
        "libraryDirectory": "src/components"
      }
    ],
    "transform-vue-jsx"
  ]
}
